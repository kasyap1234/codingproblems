class Hashtable:
    def __init__(self):

        self.max=self.max
        self.arr=[None for i in range(self.max)]
    def get_hash(self,key):
        k=0
        for i in key:
            k+=ord(i)
            return k%self.max
    def add_key_value_pair(self,key,value):
        k=self.get_hash(key)
        self.arr[k]=value
    def get_key_value_pair(self,key):
        k=self.get_hash(key)
        return  self.arr[k]
        



