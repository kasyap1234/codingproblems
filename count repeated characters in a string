#count the repeated letters in a string
def countrepeated(mystring):
    arr=[]
    count=0
    for i in mystring:
        arr.append(i)
    n=len(arr)
    for s in range(n):
       for j in range(s+1,n):
           if arr[s]==arr[j]:
               count=count +1
    return count
print(countrepeated("asap"))
